Cell["\<\
Assuming[Sin[\[Theta]] > 0, FullSimplify[Y[0, 0, \[Theta], \[Phi]]]];(*Y00*)

Assuming[Sin[\[Theta]] > 0, FullSimplify[Y[1, 0, \[Theta], \[Phi]]]];(*Y10*)
Assuming[Sin[\[Theta]] > 0, FullSimplify[(I/(\[Sqrt]2)) (Y[1, -1, \[Theta], \
\[Phi]] + Y[1, 1, \[Theta], \[Phi]])]];(*Y11p*)
Assuming[Sin[\[Theta]] > 0, FullSimplify[(1/(\[Sqrt]2)) (Y[1, -1, \[Theta], \
\[Phi]] - Y[1, 1, \[Theta], \[Phi]])]];(*Y11m*)

Assuming[Sin[\[Theta]] > 0, FullSimplify[Y[2, 0, \[Theta], \[Phi]]]];(*Y20*)
Assuming[Sin[\[Theta]] > 0, FullSimplify[(I/(\[Sqrt]2)) (Y[2, -1, \[Theta], \
\[Phi]] + Y[2, 1, \[Theta], \[Phi]])]];(*Y21p*)
Assuming[Sin[\[Theta]] > 0, FullSimplify[(1/(\[Sqrt]2)) (Y[2, -1, \[Theta], \
\[Phi]] - Y[2, 1, \[Theta], \[Phi]])]];(*Y21m*)
Assuming[Sin[\[Theta]] > 0, FullSimplify[(1/(\[Sqrt]2)) (Y[2, -2, \[Theta], \
\[Phi]] + Y[2, 2, \[Theta], \[Phi]])]];(*Y22p*)
Assuming[Sin[\[Theta]] > 0, FullSimplify[(I/(\[Sqrt]2)) (Y[2, -2, \[Theta], \
\[Phi]] - Y[2, 2, \[Theta], \[Phi]])]];(*Y22m*) 

Assuming[Sin[\[Theta]] > 0, FullSimplify[Y[3, 0, \[Theta]1, \[Phi]1]]];(*Y30*)
Assuming[Sin[\[Theta]] > 0, FullSimplify[(I/(\[Sqrt]2)) (Y[3, -1, \[Theta]1, \
\[Phi]1] + Y[3, 1, \[Theta]1, \[Phi]1])]];(*Y31p*)
Assuming[Sin[\[Theta]] > 0, FullSimplify[(1/(\[Sqrt]2)) (Y[3, -1, \[Theta]1, \
\[Phi]1] - Y[3, 1, \[Theta]1, \[Phi]1])]];(*Y31m*)
Assuming[Sin[\[Theta]] > 0, FullSimplify[(1/(\[Sqrt]2)) (Y[3, -2, \[Theta]1, \
\[Phi]1] + Y[3, 2, \[Theta]1, \[Phi]1])]];(*Y32p*)
Assuming[Sin[\[Theta]] > 0, FullSimplify[(I/(\[Sqrt]2)) (Y[3, -2, \[Theta]1, \
\[Phi]1] - Y[3, 2, \[Theta]1, \[Phi]1])]];(*Y32m*)
Assuming[Sin[\[Theta]] > 0, FullSimplify[(I/(\[Sqrt]2)) (Y[3, -3, \[Theta]1, \
\[Phi]1] + Y[3, 3, \[Theta]1, \[Phi]1])]];(*Y33p*)
Assuming[Sin[\[Theta]] > 0, FullSimplify[(1/(\[Sqrt]2)) (Y[3, -3, \[Theta]1, \
\[Phi]1] - Y[3, 3, \[Theta]1, \[Phi]1])]];(*Y33m*) 
