Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"\[ScriptCapitalN]", "[", 
     RowBox[{"k_", ",", "l_"}], "]"}], ":=", 
    RowBox[{"\[Sqrt]", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"\[Sqrt]", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"2", " ", 
            SuperscriptBox["\[Nu]", "3"]}], ")"}], "/", "\[Pi]"}], ")"}]}], 
       " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["2", 
            RowBox[{"k", "+", 
             RowBox[{"2", "l"}], "+", "3"}]], 
           RowBox[{"k", "!"}], " ", 
           SuperscriptBox["\[Nu]", "l"]}], ")"}], "/", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", "k"}], " ", "+", " ", 
            RowBox[{"2", "l"}], " ", "+", " ", "1"}], ")"}], "!!"}]}], 
        ")"}]}], ")"}]}]}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{"Normalization", " ", "Constant"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"L", "[", 
     RowBox[{"k_", ",", "l_", ",", "x_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["x", 
          RowBox[{"-", 
           RowBox[{"(", 
            RowBox[{"l", "+", 
             FractionBox["1", "2"]}], ")"}]}]], 
         RowBox[{"Exp", "[", "x", "]"}]}], ")"}], "/", 
       RowBox[{"k", "!"}]}], ")"}], 
     RowBox[{"D", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Exp", "[", 
         RowBox[{"-", "x"}], "]"}], " ", 
        SuperscriptBox["x", 
         RowBox[{"k", "+", "l", "+", 
          FractionBox["1", "2"]}]]}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "k"}], "}"}]}], "]"}]}]}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Laguerre", " ", "Polynomials"}], ",", " ", 
    RowBox[{"To", " ", "the", " ", "power", " ", "of", " ", 
     FormBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{"l", "+", 
         FractionBox["1", "2"]}], ")"}], ",", " ", 
       RowBox[{"or", " ", 
        RowBox[{"rather", ":", " ", 
         RowBox[{
          SuperscriptBox[
           SubscriptBox["L", "k"], 
           RowBox[{"(", 
            RowBox[{"l", "+", 
             FractionBox["1", "2"]}], ")"}]], "(", "x", ")"}]}]}]}],
      TraditionalForm]}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"P", "[", 
     RowBox[{"l_", ",", "m_", ",", "x_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "/", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["2", "l"], 
         RowBox[{"l", "!"}]}], ")"}]}], ")"}], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "-", 
        SuperscriptBox["x", "2"]}], ")"}], 
      RowBox[{"m", "/", "2"}]], 
     RowBox[{"D", "[", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["x", "2"], "-", "1"}], ")"}], "l"], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", 
         RowBox[{"l", "+", "m"}]}], "}"}]}], "]"}]}]}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "Associated", " ", "Legendre", " ", "Polynomials", " ", "Without", " ", 
     "the", " ", "Condon"}], "-", 
    RowBox[{
    "Shortly", " ", "phase", " ", "to", " ", "avoid", " ", "counting", " ", 
     "the", " ", "phase", " ", 
     RowBox[{"twice", "."}]}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Y", "[", 
    RowBox[{"l_", ",", "m_", ",", "\[Theta]_", ",", "\[Phi]_"}], "]"}], ":=", 
   RowBox[{"Assuming", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Sin", "[", "\[Theta]", "]"}], ">", "0"}], ",", 
