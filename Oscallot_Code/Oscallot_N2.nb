      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "m"], 
      RowBox[{"\[Sqrt]", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", "l"}], "+", "1"}], ")"}], "/", 
           RowBox[{"(", 
            RowBox[{"4", "\[Pi]"}], ")"}]}], ")"}], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"l", "-", "m"}], ")"}], "!"}], "/", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"l", "+", "m"}], ")"}], "!"}]}], ")"}]}], ")"}]}], 
      RowBox[{"P", "[", 
       RowBox[{"l", ",", "m", ",", 
        RowBox[{"Cos", "[", "\[Theta]", "]"}]}], "]"}], 
      RowBox[{"Exp", "[", 
       RowBox[{"\[ImaginaryI]", " ", "m", " ", "\[Phi]"}], "]"}]}]}], "]"}]}],
   ";", " ", 
  RowBox[{"(*", 
   RowBox[{"Spherical", " ", "Harmonics"}], "*)"}]}]}], "Program",
